        "@types/node": "^16.11.47",
        "@vitejs/plugin-vue": "^3.0.1",
        "@vitejs/plugin-vue-jsx": "^2.0.1",
        "@vue/tsconfig": "^0.1.3",
        "eslint": "^8.23.1",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-vue": "^9.4.0",
        "husky": "^8.0.0",
        "lint-staged": "^13.0.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.7.1",
        "typescript": "~4.7.4",
        "unocss": "^0.45.13",
        "unplugin-auto-import": "^0.11.2",
        "unplugin-vue-components": "^0.22.7",
