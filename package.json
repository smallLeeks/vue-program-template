{
    "name": "vue-program-template",
    "version": "0.0.0",
    "main": "electron/main/index.js",
    "scripts": {
        "dev": "run-s start",
        "start": "vite",
        "build": "run-p type-check build-only",
        "preview": "vite preview --port 4173",
        "build-only": "rimraf release dist && vite build && electron-builder",
        "type-check": "vue-tsc --noEmit",
        "lint": "eslint --cache --max-warnings 0 \"src/**/*.{vue,ts,tsx,js,jsx}\" --fix",
        "lint:staged": "lint-staged -c ./.husky/lintstagedrc.js",
        "lint:pretty": "prettier --write \"./**/*.{html,vue,ts,js,json,md}\"",
        "prepare": "husky install",
        "dts": "vue-tsc --declaration --emitDeclarationOnly"
    },
    "dependencies": {
        "@vueuse/core": "^9.1.1",
        "@xstate/vue": "^2.0.0",
        "axios": "^1.2.0",
        "element-plus": "^2.2.21",
        "pinia": "^2.0.17",
        "vue": "^3.2.37",
        "vue-router": "^4.1.3",
        "web-config": "^2.2.18",
        "xstate": "^4.33.4"
    },
    "devDependencies": {
        "@babel/core": "^7.19.1",
        "@commitlint/cli": "^17.1.2",
        "@commitlint/config-conventional": "^17.1.0",
        "@typescript-eslint/parser": "^5.38.0",
        "@vitejs/plugin-vue": "^3.0.1",
        "@vitejs/plugin-vue-jsx": "^2.0.1",
        "@vue/tsconfig": "^0.1.3",
        "electron": "^21.3.0",
        "electron-builder": "^23.6.0",
        "electron-devtools-installer": "^3.2.0",
        "eslint": "^8.23.1",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-vue": "^9.4.0",
        "husky": "^8.0.0",
        "lint-staged": "^13.0.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.7.1",
        "rimraf": "^3.0.2",
        "sass": "^1.54.9",
        "typescript": "~4.7.4",
        "unocss": "^0.45.13",
        "unplugin-auto-import": "^0.11.2",
        "unplugin-vue-components": "^0.22.7",
        "vite": "^3.0.4",
        "vite-plugin-electron": "^0.10.4",
        "vite-plugin-eslint": "^1.8.1",
        "vue-tsc": "^0.39.5",
        "vue-types": "^4.2.1"
    }
}
